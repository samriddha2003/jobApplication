{
    "version": 2,
    "builds": [
        {
            "src": "api/job-application.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/api/job-application.js" }
    ]
}
